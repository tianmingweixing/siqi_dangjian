 List<Attach> attach_img_list = attachService.getAttachByuuid(uuid,CommonString.FILETYPE_IMAGE);
            List<Attach> attach_vedio_list = attachService.getAttachByuuid(uuid,CommonString.FILETYPE_VEDIO);
            List<Map> vedio_list = new ArrayList<>();
            List<Map> imgList = new ArrayList<>();
            if (attach_img_list.size() != 0) { //如果有附件
                for (int i = 0; i < attach_img_list.size(); i++) {
                    Attach attach = attach_img_list.get(i);
                    Map map2 = new HashMap();
                    String img_base64 = CommonUtil.wirteImage(attach.getFilePath(), attach.getExtension());
                    map2.put("src", img_base64);
                    map2.put("name", attach.getFileName());
                    map2.put("id", attach.getId());
                    imgList.add(i, map2);
                }
                view.addObject("imageList", imgList);
            }